      "pStyle": "ListParagraph",
      "numPr": {
        "numId": "3",
        "ilvl": "0"
      },
      "pPr_hints": null,
      "contains_sectPr": false
    },
    {
      "paragraph_index": 5,
      "text": "Joint sealants for interior and exterior applications.",
      "pStyle": "ListParagraph",
      "numPr": {
        "numId": "4",
        "ilvl": "0"
      },
      "pPr_hints": null,
      "contains_sectPr": false
    },
    {
      "paragraph_index": 6,
      "text": "Joint fillers and backing materials.",
      "pStyle": "ListParagraph",
      "numPr": {
        "numId": "4",
        "ilvl": "0"
      },
      "pPr_hints": null,
      "contains_sectPr": false
    },
    {
      "paragraph_index": 7,
      "text": "Surface preparation and priming.",
      "pStyle": "ListParagraph",
      "numPr": {
        "numId": "4",
        "ilvl": "0"
      },
      "pPr_hints": null,
      "contains_sectPr": false
